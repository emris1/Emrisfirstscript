-- loadstring

local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

 

-- config

ESP.Players = false

ESP.Boxes = false

ESP.Names = true

ESP:Toggle(true)

 

-- object

ESP:AddObjectListener(Workspace .DynamicFurniture, {

    Name = "ScrapPile",

    CustomName = "scrap",

    Color = Color3.fromRGB(0, 255, 0),

    IsEnabled = "whatever"

})

ESP.whatever = true



